{
    "name": "kiboit/phast",
    "description": "A toolbox for optimizing web page performance",
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kiboit/jsmin-php"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "facebook/webdriver": "^1.5",
        "ext-dom": "*",
        "friendsofphp/php-cs-fixer": "^2.16",
        "kiboit/jsmin-php": "dev-master",
        "nikic/php-parser": "^4.13",
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
        "nikic/php-parser": "Needed for compilation"
    },
    "autoload": {
        "psr-4": {
            "Kibo\\Phast\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kibo\\Phast\\": [
                "test/php/"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
